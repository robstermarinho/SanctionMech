package sm;

import jamder.Actions;
import jamder.Agent;
import jamder.AgentRole;
import jamder.Norms;

public class No {

	
	boolean generico = true;	//Se for um no generico (= false)
	
	No pai = null;											
    No esquerda = null;  
    No direita = null;  
    
    int valor;
    Actions acao;
    Agent agente;

    public No(int valor, Actions acao) {
        this.valor = valor;
        this.acao = acao;
        this.generico = true;
    }
    
    public No() {
    	this.acao = null;
    	this.valor = -1;
        this.generico = false;  
    }
    
}
