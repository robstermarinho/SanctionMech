package sm;

import jamder.Actions;
import jamder.Agent;
import jamder.AgentRole;
import jamder.Norms;

public class No {

	
	boolean generico = true;	//Se for um no generico (= false)
	
	No pai = null;											
    No esquerda = null;  
    No direita = null;  
    
    int valor;
    Actions acao;
    Norms norma;

    public No(int valor) {
        this.valor = valor;
        this.generico = true;
    }
    
    public No() {
    	this.valor = -1;
        this.generico = false;  
    }
    
}
