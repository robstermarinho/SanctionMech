package jamder;

public class Norms {
	
	Actions action;
	int sanction_cost;
	Agent agente;
	
	public Norms(Agent agente, Actions action, Norms sanction, int sanction_cost) {
		super();
		this.agente = agente;
		this.action = action;
		this.sanction_cost = sanction_cost;
	}

	public Actions getAction() {
		return action;
	}

	public void setAction(Actions action) {
		this.action = action;
	}

	public int getSanction_cost() {
		return sanction_cost;
	}

	public void setSanction_cost(int sanction_cost) {
		this.sanction_cost = sanction_cost;
	}
	
	
	
}
