package jamder.norms;

public enum NormType {
	PERMISSION("Permission"),
	PROHIBITION("Prohibition"),
	OBLIGATION("Obligation");
	
	private String type;
	
	NormType(String type) {
		this.type = type;
	}

	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
}