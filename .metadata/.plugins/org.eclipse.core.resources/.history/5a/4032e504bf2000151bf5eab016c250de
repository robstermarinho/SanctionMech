package sm;

public class Custos {
	int Cvmf;       //Custo de violçao da acao final
	int Cvi;        //Custo de violação individual
	int Cvc;        //Custo de violaçao coletiva	
	int Cvg;        //Custo de violaçao gerencial
	
	
	public Custos() {
		super();
		Cvmf = 0;
		Cvi = 0;
		Cvc = 0;
		Cvg = 0;
	}


	public int getCvi() {
		return Cvi;
	}


	public void setCvi(int cvi) {
		Cvi = cvi;
	}


	public int getCvc() {
		return Cvc;
	}


	public void setCvc(int cvc) {
		Cvc = cvc;
	}


	public int getCvg() {
		return Cvg;
	}


	public void setCvg(int cvg) {
		Cvg = cvg;
	}
	

	
}
