		arvore.inserir_novo(0,1);
		arvore.inserir_novo(0,2);
		arvore.inserir_novo(2,22);
		arvore.inserir_novo(2,23);
		arvore.inserir_novo(1,11);
		arvore.inserir_novo(1,12);
		arvore.inserir_novo(1,13);
		
